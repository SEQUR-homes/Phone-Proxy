{
  "name": "sequr-phone-proxy",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint src --ext .ts",
    "format": "prettier src -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "tsc",
    "start": "twilio-run start",
