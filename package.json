    "@twilio/runtime-handler": "1.2.0",
    "twilio": "^3.56"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "2.4.1",
    "twilio-run": "^3.3.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "12"
  }
}
